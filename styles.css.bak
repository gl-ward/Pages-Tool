body{
	background-color: rgb(0, 99, 158)!important;
}
.mainBody{
	background-color: white;
}
.container{
	width:100%;
	padding-right: 0px;
	padding-left: 0px;
}
.col-md-3{
	margin-top: 10px;
}
.well{
	border-radius: 35%!important;
	color: #fff!important;
	height: 140px;
	width: 150px;
	font-weight: bold;
	text-align: center;
	background-color: rgb(176, 64, 38)!important;
	padding-top: 55px!important;
	padding: 0px;
	font-size: 20px;
	cursor: pointer;
	box-shadow: 0 3px 10px rgb(0 0 0 / 58%);
}
.headerImage{
	height: 100px;
    margin-left: -15px;
    float: right;
}
.headerText{
	float:left;
	color:white;
	font-size: 60px;
}
.textBody{
	width: 90%;
    padding-left: 18%;
    text-align: left;
}
#twoblocks{
	padding-left: 72px;
}
@media (min-width: 992px){
	.col-md-3 {
		width: 20%;
	}
}
@media (max-width: 1000px) {
	.well{
		float: none;
		margin: 0 auto;
		margin-bottom: 10px;
		margin-top: 10px;
	}
	.headerImage{
		float:left;
		padding-left: 25%;
	}
	.headerText{
		float:right;
		padding-right: 5%;
	}
	#twoblocks{
		padding-left: 15px;
	}
}
@media (max-width: 700px) {
	.headerImage{
		float: none;
		margin: 0 auto;
		padding-left: 0%;
	}
	.headerText{
		float: none;
		margin: 0 auto;
		padding-right: 0%
	}
}